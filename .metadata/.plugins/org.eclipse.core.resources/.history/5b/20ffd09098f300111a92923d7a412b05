package IOStream;

import java.io.IOException;
import java.io.ObjectInputStream;
import java.net.Socket;
import java.util.Vector;
import Data.PayloadData;

public class PayloadObjectRX 
{
	public Vector<PayloadData> payloadDataVector;
	private  ObjectInputStream objectInputStream; 
	public Socket socket;
	
	public   Vector<PayloadData> getPayloadObject(Socket _socket)	
	{
		this.socket = _socket;
		try 
		{
			objectInputStream = new ObjectInputStream(socket.getInputStream());
			
			try 
			{
				Object updatePayloadData = 	(PayloadData) objectInputStream.readUnshared();
				payloadDataVector.addElement((PayloadData) updatePayloadData);
			} 
			catch (ClassNotFoundException e) 
			{
				e.printStackTrace();
			}
			
	    } 
		catch (IOException e) 
		{
			e.printStackTrace();
		}
		return payloadDataVector;
	}
}
